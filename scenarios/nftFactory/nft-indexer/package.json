{
  "name": "nftFactory",
  "version": "0.1.0",
  "scripts": {
    "test": "tsc --build && mocha build/test --exit",
    "clean": "tsc --clean",
    "build": "tsc --build",
    "watch": "tsc --watch",
    "gen:clean": "(cd generated && rescript clean )",
    "gen:build": "(cd generated && rescript build)",
    "gen:watch": "(cd generated && rescript build -w)",
    "start": "tsc --build && node build/generated/src/Index.bs.js",
    "codegen-fuji": "cargo run --manifest-path ../../../codegenerator/Cargo.toml -- codegen -c config.fuji.yaml",
    "codegen": "cargo run --manifest-path ../../../codegenerator/Cargo.toml -- codegen"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "generated": "workspace:*"
  }
}
