{
  "name": "gravatar",
  "version": "0.1.0",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "test",
      "subdirs": true
    }
  ],
  "suffix": ".bs.js",
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "gentypeconfig": {
    "language": "typescript",
    "generatedFileExtension": ".gen.ts",
    "module": "commonjs",
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "bs-dependencies": [
    "generated",
    "@ryyppy/rescript-promise",
    "rescript-mocha",
    "@greenlabs/ppx-spice"
  ],
  "pinned-dependencies": [
    "generated"
  ],
  "ppx-flags": [
    "@greenlabs/ppx-spice/ppx"
  ]
}
