{{#each codegen_contracts as |contract|}}
  let register{{contract.name.capitalized
  }}Handlers = () => {
try {
  let _ = %raw(`require("{{contract.handler.relative_to_generated_src
  }}")`)
  } catch {
  | err => { Logging.error("EE500: There was an issue importing the handler file for {{contract.name.capitalized
  }}. Expected file to parse at {{contract.handler.relative_to_config
  }}")
  Js.log(err)
  }
  }
}
{{/each}}

let registerAllHandlers = () => {
{{#each codegen_contracts as |contract|}}
  register{{contract.name.capitalized}}Handlers()
{{/each}}
}
