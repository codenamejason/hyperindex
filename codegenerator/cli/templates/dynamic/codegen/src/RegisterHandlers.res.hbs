{{#each contracts as |contract|}}
  let register{{contract.name.capitalized
  }}Handlers = () => {
try {
  let _ = %raw(`require("{{contract.handler.relative_to_generated_src
  }}")`)
  } catch {
  | _ => Logging.error("Unable to import the handler file for {{contract.name.capitalized
  }}. Expected a file at {{contract.handler.relative_to_generated_src
  }}, make sure this file is parsing as well")
  }
}
{{/each}}

let registerAllHandlers = () => {
{{#each contracts as |contract|}}
  register{{contract.name.capitalized}}Handlers()
{{/each}}
}
