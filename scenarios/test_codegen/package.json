{
  "name": "gravatar",
  "version": "0.1.0",
  "scripts": {
    "ts:clean": "tsc --clean",
    "ts:build": "hardhat compile && tsc --build",
    "ts:watch": "tsc --watch",
    "gen:clean": "(cd generated && rescript clean )",
    "gen:build": "(cd generated && rescript build)",
    "gen:watch": "(cd generated && rescript build -w)",
    "res:clean": "rescript clean -with-deps",
    "res:build": "rescript build -with-deps",
    "res:start": "node generated/src/Index.bs.js",
    "res:watch": "rescript build -w -with-deps",
    "test": "rescript build && mocha",
    "test:log": "DEBUG=true npm test",
    "codegen": "cargo run --manifest-path ../../codegenerator/Cargo.toml -- codegen"
  },
  "keywords": [
    "ReScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/rescript-jest": "^0.9.2",
    "@nomiclabs/hardhat-ethers": "^3.0.0-beta.0",
    "@ryyppy/rescript-promise": "^2.1.0",
    "@typechain/ethers-v6": "^0.3.2",
    "@typechain/hardhat": "^6.1.6",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "ethers": "^6.4.0",
    "hardhat": "^2.13.1",
    "mocha": "^10.2.0",
    "mocha-reporter": "^0.1.1",
    "rescript": "^10.1.2",
    "rescript-mocha": "^0.9.0",
    "sinon": "^15.0.4",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "generated": "workspace:*",
    "typescript": "^5.0.4"
  }
}
