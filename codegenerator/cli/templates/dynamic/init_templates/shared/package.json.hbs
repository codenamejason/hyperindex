{
  "name": "{{project_name}}",
  "version": "0.1.0",
  "scripts": {
  {{#if is_typescript}}
    "clean": "tsc --clean",
    "build": "tsc --build",
    "watch": "tsc --watch",
  {{/if}}
  {{#if is_rescript}}
    "clean": "rescript clean -with-deps",
    "build": "rescript build -with-deps",
    "watch": "rescript build -w -with-deps",
  {{/if}}
    "codegen": "envio codegen",
    "dev": "{{#if is_rescript}}pnpm build && {{/if}}envio dev",
    "test": "{{#if is_rescript}}pnpm build && {{/if}}mocha {{#if is_typescript}}--require ts-node/register test/**/*.ts{{else}}test/**/*.js{{/if}}",
    "start": "{{#if is_rescript}}pnpm build && {{/if}}{{#if is_typescript}}ts-{{/if}}node generated/src/Index.bs.js"
  },
  "devDependencies": {
  {{#if is_rescript}}
    "@glennsl/rescript-fetch": "0.2.0",
    "@greenlabs/ppx-spice": "0.1.14",
    "@ryyppy/rescript-promise": "2.1.0",
    "express": "4.18.2",
    "rescript-envsafe": "1.3.0",
    "rescript-express": "0.4.1",
    "rescript-struct": "4.1.1",
    "rescript-mocha": "^0.9.0",
  {{/if}}
  {{#if is_typescript}}
    "@types/node": "20.8.8",
    "typescript": "5.2.2",
    "@types/chai": "4.3.11",
    "@types/mocha": "10.0.6",
  {{/if}}    
    "chai": "4.3.10",
    "ethers": "6.8.0",
    "mocha": "10.2.0",
    "rescript": "10.1.4"
  },
  "dependencies": {
  {{#if is_typescript}}
    "ts-node": "10.9.1",
  {{/if}}
    "envio": "{{envio_version}}"
  }
}
