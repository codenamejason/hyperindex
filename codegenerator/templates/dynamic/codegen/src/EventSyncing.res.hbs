let startSyncingAllEvents = () => {
  let chainManager: ChainManager.t = ChainManager.make(~configs=Config.config, ~maxQueueSize=10000)
  let logger = Logging.createChild(~params={"event_processor": "main"})

  logger->Logging.childInfo("Starting chain fetchers.")
  chainManager->ChainManager.startFetchers->ignore

  logger->Logging.childInfo("Starting main event processer.")

  EventProcessing.startProcessingEventsOnQueue(~chainManager, ~logger)->ignore
}
