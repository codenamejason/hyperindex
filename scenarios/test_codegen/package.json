{
  "name": "gravatar",
  "version": "0.1.0",
  "scripts": {
    "clean": "rescript clean -with-deps",
    "build": "rescript build -with-deps",
    "start": "node generated/src/Index.bs.js",
    "watch": "rescript build -w -with-deps",
    "test-demo": "node --watch ./src/tests/Test.bs.js",
    "test": "rescript build && mocha --exit",
    "bench": "jest --projects ./jest-bench.config.json",
    "codegen": "cargo run --manifest-path ../../codegenerator/Cargo.toml -- codegen"
  },
  "keywords": [
    "ReScript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@glennsl/rescript-jest": "^0.9.2",
    "@nomiclabs/hardhat-ethers": "^3.0.0-beta.0",
    "@ryyppy/rescript-promise": "^2.1.0",
    "chai": "^4.3.7",
    "hardhat": "^2.13.1",
    "mocha": "^10.2.0",
    "rescript": "^10.1.2",
    "rescript-mocha": "^0.9.0",
    "sinon": "^15.0.4"
  },
  "dependencies": {
    "generated": "workspace:*"
  }
}
