{
  "name": "gravatar",
  "version": "0.1.0",
  "uncurried": false,
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "test",
      "subdirs": true
    }
  ],
  "suffix": ".bs.js",
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "gentypeconfig": {
    "language": "typescript",
    "generatedFileExtension": ".gen.ts",
    "module": "commonjs",
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "jsx": {
    "version": 4
  },
  "bs-dependencies": [
    "@rescript/react",
    "generated",
    "@ryyppy/rescript-promise",
    "rescript-mocha",
    "rescript-nodejs",
    "rescript-schema",
    "@dzakh/rescript-ava",
    "helpers"
  ],
  "bsc-flags": ["-open RescriptSchema"]
}
