{
  "name": "greeter",
  "version": "0.1.0",
  "gentypeconfig": {
    "language": "typescript",
    "shims": {},
    "generatedFileExtension": ".gen.ts",
    "module": "commonjs",
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "generated",
      "subdirs": true
    }
  ],
  "suffix": ".bs.js",
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "bs-dependencies": [
    "@ryyppy/rescript-promise",
    "rescript-envsafe",
    "rescript-struct",
    "@greenlabs/ppx-spice",
    "rescript-express"
  ],
  "ppx-flags": [
    "@greenlabs/ppx-spice/ppx"
  ],
  "bsc-flags": [
    "-open RescriptStruct"
  ]
}
